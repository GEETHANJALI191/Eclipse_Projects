package PACK1;

public class Difference {
	public static void main(String[] args) {
		
	}

	static int maxDifference(int[] a)
	{
		int max=a[0],count=0;
		for(int i=1;i<a.length;i++)
		{
			count++;
			if(a[i]>max){
				max=a[i];
		}
			int min=a[0];
			for( i=1;i<count;i++)
			{
				if(a[i]<min)
				{
					min=a[i];
			    }
			}
			
	
	


